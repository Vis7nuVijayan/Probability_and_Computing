
public class Solution
{
	static double[] getRandomRating(int n)
	{
		double temp[] = new double[n];
		for(int i=0; i<n; ++i)
			temp[i] = Math.random();
		return temp;
	}
	
	public static void main(String args[])
	{
		double[] rating = getRandomRating(3);
		for(int i=0; i<3; ++i)
			System.out.print(rating[i]+" ");
		System.out.println();
	}
}
